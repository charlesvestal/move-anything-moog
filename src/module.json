{
  "id": "moog",
  "name": "RaffoSynth",
  "abbrev": "RF",
  "version": "0.2.0",
  "description": "Monophonic synthesizer based on RaffoSynth (4 oscillators, Moog ladder filter)",
  "author": "Nicolas Roulet, Julian Palladino (port: charlesvestal)",
  "license": "MIT",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": false,
    "aftertouch": true,
    "chainable": true,
    "component_type": "sound_generator",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "RaffoSynth",
          "params": ["preset", "cutoff", "resonance", "contour", "attack", "decay", "sustain", "release", "glide", "osc1_wave", "osc2_wave", "osc2_detune", "noise"],
          "knobs": ["cutoff", "resonance", "contour", "attack", "decay", "sustain", "release", "glide"]
        }
      }
    },
    "chain_params": [
      {
        "key": "preset",
        "name": "Preset",
        "type": "int",
        "min": 0,
        "max_param": "preset_count",
        "default": 0
      },
      {
        "key": "cutoff",
        "name": "Cutoff",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.7,
        "step": 0.02
      },
      {
        "key": "resonance",
        "name": "Resonance",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.2,
        "step": 0.02
      },
      {
        "key": "contour",
        "name": "Contour",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.3,
        "step": 0.02
      },
      {
        "key": "attack",
        "name": "Attack",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.01,
        "step": 0.02
      },
      {
        "key": "decay",
        "name": "Decay",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.3,
        "step": 0.02
      },
      {
        "key": "sustain",
        "name": "Sustain",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.7,
        "step": 0.02
      },
      {
        "key": "release",
        "name": "Release",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.2,
        "step": 0.02
      },
      {
        "key": "glide",
        "name": "Glide",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.02
      },
      {
        "key": "noise",
        "name": "Noise",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.02
      },
      {
        "key": "osc1_volume",
        "name": "Osc1 Vol",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.8,
        "step": 0.02
      },
      {
        "key": "osc2_volume",
        "name": "Osc2 Vol",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.02
      },
      {
        "key": "osc3_volume",
        "name": "Osc3 Vol",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.02
      }
    ]
  }
}
